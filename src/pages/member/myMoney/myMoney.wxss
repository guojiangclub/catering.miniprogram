.moneyContainer .money-bg {
  width: 100%;
}
.moneyContainer .content {
  position: absolute;
  width: 100%;
  top: 35px;
  padding: 0 20px;
}
.moneyContainer .content .card-box {
  width: 100%;
  height: 165px;
  background: url("https://cdn.ibrand.cc/shitang94939rr.png") no-repeat center;
  background-size: cover;
  padding: 22px 0;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.moneyContainer .content .card-box .user-info {
  position: relative;
  display: flex;
  align-items: center;
  padding-left: 14px;
  color: #ffffff;
}
.moneyContainer .content .card-box .user-info .name-box {
  padding-left: 20px;
}
.moneyContainer .content .card-box .user-info .name-box .tag {
  font-size: 17px;
}
.moneyContainer .content .card-box .user-info .charge {
  width: 64px;
  height: 20px;
  border-radius: 10px;
  background: rgba(255, 255, 255, 0.3);
  font-size: 12px;
  text-align: center;
  margin-left: 152px;
}
.moneyContainer .content .card-box .money {
  font-size: 40px;
  margin-left: 34px;
  color: #fff;
}
.moneyContainer .content .title-box {
  text-align: center;
  margin-top: 13px;
}
.moneyContainer .content .title-box .title-line {
  height: 1px;
  width: 100%;
  background: #CCCCCC;
}
.moneyContainer .content .title-box .title {
  position: relative;
  display: inline-block;
  top: -13px;
  font-size: 16px;
  color: #666666;
  background: #FFFFFF;
  padding: 0px 10px;
}
.moneyContainer .content .privilege-box {
  padding: 20px 10px;
  background: #ffffff;
  box-shadow: 0px 0px 18px 0px rgba(204, 204, 204, 0.5);
  position: relative;
}
.moneyContainer .content .privilege-box .charge-list {
  width: 100%;
  height: 50px;
  border-bottom: 1px solid #E8E9E8;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 12px;
}
.moneyContainer .content .privilege-box .charge-list .charge-type {
  font-size: 15px;
  color: #333333;
}
.moneyContainer .content .privilege-box .charge-list .charge-date {
  font-size: 12px;
  color: #999999;
}
.moneyContainer .content .privilege-box .charge-list .charge-num {
  color: #FF8D00;
  color: 15px;
}
.moneyContainer .content .privilege-box .charge-list:last-child {
  margin-bottom: 0;
}
