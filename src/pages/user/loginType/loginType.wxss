#user-register,page{background-color:#fff}#user-register{font-size:16px;padding:0 15px;margin-top:50px}#user-register .tips{position:fixed;top:0;left:0;right:0;text-align:center;line-height:35px;background-color:#e64340;height:35px;color:#FFF}#user-register .register_header{padding:36px 0 56px;text-align:center;color:#ff2741;font-size:30px}#user-register .register_input .code .writecode .input-placeholder,#user-register .register_input .tellphone .input-placeholder{color:#9B9B9B;font-size:14px}#user-register .register_input{margin-bottom:60px}#user-register .register_input .tellphone{background:rgba(155,155,155,.3);margin-bottom:20px;border-radius:4px}#user-register .register_input .tellphone input{padding:15px 0 15px 15px;border-radius:4px}#user-register .register_input .code{display:flex;align-items:center;justify-content:space-between}#user-register .register_input .code .writecode{width:60%;background:rgba(155,155,155,.3);border-radius:4px}#user-register .register_input .code .writecode input{padding:15px 0 15px 15px}#user-register .register_input .code .getCode{padding:15px 0;flex:1;margin-left:17px;background-color:#ff2741;text-align:center;color:#FFF;border-radius:4px;font-size:14px}#user-register .select{display:flex;align-items:center;justify-content:center;margin-top:15px;color:#4A4A4A;font-size:12px}#user-register .select checkbox .wx-checkbox-input{background-color:#fff;border:1px solid #c1caca;box-shadow:0 1px 2px rgba(0,0,0,.05),inset 0 -15px 10px -12px rgba(0,0,0,.05);height:14px;width:14px}#user-register .select checkbox .wx-checkbox-input.wx-checkbox-input-checked{background-color:#ff2741;border:1px solid #ff2741}#user-register .submit button{color:#fff;padding:10px 0;border:none;border-radius:4px;background-color:#C9C8C8;font-size:16px}#user-register .submit button.check{background:#ff2741}#user-register .submit button:after{border:none}