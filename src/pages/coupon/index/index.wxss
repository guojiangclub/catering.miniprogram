#transfer .coupon-box .mask {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.7);
}
#transfer .coupon-box .coupon-body {
  position: fixed;
  width: 90%;
  /*height: 71%;*/
  border-radius: 5px;
  background-color: #ff2741;
  top: 50%;
  left: 50%;
  padding: 0 25px;
  transform: translate(-50%, -50%);
  box-sizing: border-box;
  z-index: 99;
}
#transfer .coupon-box .coupon-body .title {
  text-align: center;
  color: #FFFFFF;
  font-size: 22px;
  margin-top: 25px;
}
#transfer .coupon-box .coupon-body .tips {
  margin-top: 35px;
  line-height: 20px;
  color: #FFFFFF;
  font-size: 13px;
}
#transfer .coupon-box .coupon-body .coupon {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 15px 0;
  padding: 0 15px;
  width: 100%;
  height: 60px;
  background: url("http://ibrand-miniprogram.oss-cn-hangzhou.aliyuncs.com/18-11-19/7512419.jpg") no-repeat;
  background-size: 100% 100%;
  box-sizing: border-box;
}
#transfer .coupon-box .coupon-body .coupon .left {
  display: flex;
  overflow: hidden;
  flex: 1;
}
#transfer .coupon-box .coupon-body .coupon .left .icon {
  background: #ff2741;
  width: 40px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  border-radius: 100%;
}
#transfer .coupon-box .coupon-body .coupon .left .icon i {
  color: #FFFFFF;
  font-size: 25px;
}
#transfer .coupon-box .coupon-body .coupon .left .text-box {
  margin-left: 10px;
  overflow: hidden;
  flex: 1;
}
#transfer .coupon-box .coupon-body .coupon .left .text-box .title {
  text-align: left;
  font-size: 16px;
  color: #202020;
  margin-top: 0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  /*margin-bottom: 10px;*/
}
#transfer .coupon-box .coupon-body .coupon .left .text-box .info {
  font-size: 10px;
  color: #909090;
}
#transfer .coupon-box .coupon-body .coupon .right {
  color: #ff2741;
  font-size: 20px;
  font-weight: 600;
}
#transfer .coupon-box .coupon-body .coupon .right .money {
  font-size: 14px;
  font-weight: 100;
}
#transfer .coupon-box .coupon-body .user-tips {
  color: #ffffff;
  font-size: 10px;
}
#transfer .coupon-box .coupon-body .btn {
  margin: 40px 0 35px 0;
  width: 100%;
  height: 50px;
  line-height: 50px;
  box-shadow: 0px 4px 8px 2px rgba(0, 0, 0, 0.2);
  border-radius: 4px;
  background: #FFFFFF;
  color: #B24B4B;
  text-align: center;
}
