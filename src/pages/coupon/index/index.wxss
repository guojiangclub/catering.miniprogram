#coupon-index .navbar {
  position: fixed;
  top: 0;
  width: 100%;
  display: flex;
  z-index: 20;
  height: 50px;
  line-height: 50px;
  background: #FFFFFF;
  font-size: 14px;
  color: #9B9B9B;
}
#coupon-index .navbar .navbar-item {
  flex: 1;
  width: 0%;
  text-align: center;
}
#coupon-index .navbar .navbar-item.activity {
  color: #ff2741;
}
#coupon-index .navbar .navbar-slider {
  position: absolute;
  content: " ";
  left: 0;
  bottom: 0;
  width: 4em;
  height: 3px;
  background-color: #ff2741;
  transition: transform .3s;
}
#coupon-index .tab-panel {
  padding: 10px 15px 50px 15px;
}
#coupon-index .tab-panel .tab-content .coupon {
  display: flex;
  margin-bottom: 10px;
}
#coupon-index .tab-panel .tab-content .coupon.shasow {
  box-shadow: 0px 7px 10px 0px rgba(6, 0, 0, 0.2);
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left {
  width: 110px;
  height: 80px;
  color: #ffffff;
  background-size: cover;
  background: #ff2741;
  position: relative;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left.discount {
  background: #F8B62D;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left .text-wrap {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100%;
  font-size: 12px;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left .text-wrap .text-box {
  text-align: center;
  width: 100%;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left .text-wrap .text-box .text {
  display: block;
  padding: 0 10px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 20px;
  font-weight: 600;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left .text-wrap .text-box .text.label {
  font-size: 12px;
  font-weight: 100;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left .text-wrap .text-box .text .money {
  font-size: 14px;
  font-weight: 100;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left .text-wrap .text-box .money-num {
  font-size: 25px;
  font-weight: bold;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left .dot-wrap {
  position: absolute;
  top: -12px;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left .dot-wrap .dot-item {
  width: 5px;
  height: 5px;
  background: #F3F3F3;
  border-radius: 50%;
  margin: 11px 0 9px -2px;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-left .dot-wrap .dot-item:last-child {
  margin-top: 10px;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-right {
  flex: 1;
  background: #fff;
  padding: 0px 15px;
  font-size: 12px;
  overflow: hidden;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-right .top {
  font-size: 12px;
  display: -webkit-box;
  color: #333333;
  height: 40px;
  line-height: 20px;
  padding-top: 8px;
  box-sizing: border-box;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-right .top .type {
  color: #333333;
  border: 1px solid #333333;
  padding: 2px 5px;
  border-radius: 3px;
  line-height: 20px;
  margin-right: 5px;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-right .bottom .bottom-use {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 12px;
  margin: 7px 0;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-right .bottom .bottom-use .tiem-box {
  color: #9E9E9F;
  flex: 1;
  font-size: 9px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 5px;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-right .bottom .bottom-use .btn {
  text-align: center;
  border-radius: 3px;
  padding: 3px 12px;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-right .bottom .bottom-use .use {
  color: #fff;
  border: 1px solid #ff2741;
  background: #ff2741;
}
#coupon-index .tab-panel .tab-content .coupon .coupon-right .bottom .bottom-use .already {
  color: #ff2741;
  background: #fff;
  border: 1px solid #ff2741;
}
#coupon-index .tab-panel .tab-content .no-list {
  text-align: center;
  font-size: 14px;
  color: #888888;
  margin-top: 10px;
}
#coupon-index .tab-panel .tab-content .no-list i {
  font-size: 80px;
  margin-bottom: 15px;
}
#coupon-index .see-over {
  position: fixed;
  bottom: 0px;
  width: 100%;
  text-align: center;
  background-color: #DCDCDC;
  padding: 12px 0;
  font-size: 12px;
  color: #4A4A4A;
}
